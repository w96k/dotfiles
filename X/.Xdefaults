URxvt.termName: rxvt

!TomorrowNightEighties
*.foreground:   #cccccc
*.background:   #2d2d2d
*.cursorColor:  #f2777a

! Black / Grey
*.color0:       #999999
*.color8:       #666666

! Red / Bright Red
*.color1:       #f2777a
*.color9:       #FF3334

! Green + Bright Green
*.color2:       #99cc99
*.color10:      #9ec400

! Yellow (Orange) + Bright Yellow (Yellow)
*.color3:       #f99157
*.color11:      #ffcc66

! Blue + Bright Blue
*.color4:       #6699cc
*.color12:      #6699cc

! Magenta (Purple) + Bright Magenta
*.color5:       #cc99cc
*.color13:      #b777e0

! Cyan (Aqua) + Bright Cyan
*.color6:       #66cccc
*.color14:      #54ced6

! Light Grey (Selection) + White (Current Line)
*.color7:       #515151
*.color15:      #393939

! URxvt
URxvt.font: xft:terminus:pixelsize=14
URxvt.scrollBar: false
URxvt.underlineURLs:  True
URxvt.cursorBlink: True
Urxvt.cursorUnderline: True
Urxvt.visualBell: True
Urxvt.fading: 50
Urxvt.fadeColor: #ff0000

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 0
Xft.antialias: 1
Xft.rgba: rgb

! Change cursor
Xcursor.theme: Adwaita