URxvt.termName: rxvt-256color

!TomorrowNightEighties
*.foreground:   #cccccc
*.background:   #2d2d2d
*.cursorColor:  #f2777a

! Black / Grey
*.color0:       #999999
*.color8:       #666666

! Red / Bright Red
*.color1:       #f2777a
*.color9:       #FF3334

! Green + Bright Green
*.color2:       #99cc99
*.color10:      #9ec400

! Yellow (Orange) + Bright Yellow (Yellow)
*.color3:       #f99157
*.color11:      #ffcc66

! Blue + Bright Blue
*.color4:       #6699cc
*.color12:      #6699cc

! Magenta (Purple) + Bright Magenta
*.color5:       #cc99cc
*.color13:      #b777e0

! Cyan (Aqua) + Bright Cyan
*.color6:       #66cccc
*.color14:      #54ced6

! Light Grey (Selection) + White (Current Line)
*.color7:       #515151
*.color15:      #393939

! Xterm
XTerm.vt100*faceName: Fira Code
XTerm*faceSize: 9
XTerm*renderFont: true

! URxvt
URxvt.font: xft: Fira Mono:style=Regular:size=10.5
URxvt.boldFont: xft:Fira Mono:style=Bold:size=10.5
URxvt.italicFont: xft:Fira Mono:style=Light:size=10.5
URxvt.letterSpace: -1
URxvt.scrollBar: false
URxvt.urlLauncher: icecat
URxvt.underlineURLs:  True
URxvt.cursorBlink: True
nUrxvt.cursorUnderline: True
Urxvt.visualBell: True
Urxvt.fading: 50
Urxvt.fadeColor: #ff0000

! Emacs
Emacs.fontBackend: xft
Emacs.font: Fira Code:size=14

! Font Settings
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
! Change cursor
Xcursor.theme: Adwaita

! Rofi

! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: #333333,  #cccccc,  #333333,  #f2777a,  #333333
rofi.color-urgent: #333333,  #cccccc,  #333333,  #2d2d2d,  #cccccc
rofi.color-active: #333333,  #cccccc,  #333333,  #2d2d2d,  #cccccc

!                  'background', 'border', 'separator'
rofi.color-window: #666666,      #666666,  #666666
